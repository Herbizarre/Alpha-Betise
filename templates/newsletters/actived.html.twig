{% extends 'base.html.twig' %}

{% block title %}Hello NewslettersController!{% endblock %}

{% block body %}

<div class="card mb-3 border-primary">
		<div class="row g-0">
			<div class="col-md-4 m-auto">

				{% for message in app.flashes('message') %}
					<div class="CpActived">
						{{ message | upper}}
					</div>
				{% endfor %}

			</div>
		</div>
	</div>

{% endblock %}